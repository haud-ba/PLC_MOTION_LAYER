    <Method Name="AxisDisable" Id="{5b07de5c-833d-0c24-00ad-ad559c3149a3}" FolderPath="methods\">
      <Declaration><![CDATA[METHOD AxisDisable : E_PROGRESS
VAR_INPUT
  Execute           : BOOL;
END_VAR
VAR_INST
  _eState,
  _eResult          : E_PROGRESS;
  _rValue           : LREAL;
  _udiErr           : UDINT;
  _stMsg            : ST_Message;


END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT Check() THEN AxisDisable := E_PROGRESS.ERROR; RETURN; END_IF
IF NOT Execute THEN _eState := E_PROGRESS.INIT; END_IF

CASE _eState
OF
  E_PROGRESS.INIT:
    IF Execute 
    THEN
      memset(ADR(_stMsg),0,SIZEOF(_stMsg));
      _udiErr                       := 0;
      _rValue                       := 0;
      _eState                       := E_PROGRESS.BUSY;
    END_IF
END_CASE
CASE _eState
OF
  E_PROGRESS.BUSY:
    _eResult                        := Disable(FALSE);
    _eState                         := E_PROGRESS.WAITING;
END_CASE
CASE _eState
OF
  E_PROGRESS.WAITING:
    _eResult                        := Disable(TRUE);

    IF (_eResult = E_PROGRESS.DONE)
    THEN
      _eState                       := E_PROGRESS.WORKING;
    END_IF
END_CASE
CASE _eState
OF
  E_PROGRESS.WORKING:
    _eState                         := E_PROGRESS.DONE;
END_CASE

AxisDisable := _eState;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="AxisEnable" Id="{bdedff7a-4ac5-01d7-2a4e-56553b33f7f1}" FolderPath="methods\">
      <Declaration><![CDATA[METHOD AxisEnable : E_PROGRESS
VAR_INPUT
  Execute           : BOOL;
END_VAR
VAR_INST
  _eState,
  _eResult          : E_PROGRESS;
  _rValue           : LREAL;
  _udiErr           : UDINT;
  _stMsg            : ST_Message;


END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT Check() THEN AxisEnable := E_PROGRESS.ERROR; RETURN; END_IF
IF NOT Execute THEN _eState := E_PROGRESS.INIT; END_IF

CASE _eState
OF
  E_PROGRESS.INIT:
    IF Execute 
    THEN
      memset(ADR(_stMsg),0,SIZEOF(_stMsg));
      _udiErr                       := 0;
      _rValue                       := 0;
      _eState                       := E_PROGRESS.BUSY;
    END_IF
END_CASE
CASE _eState
OF
  E_PROGRESS.BUSY:
    _eResult                        := Enable(FALSE);
    _eState                         := E_PROGRESS.WAITING;
END_CASE
CASE _eState
OF
  E_PROGRESS.WAITING:
    _eResult                        := Enable(TRUE);

    IF (_eResult = E_PROGRESS.DONE)
    THEN
      _eState                       := E_PROGRESS.WORKING;
    END_IF
END_CASE
CASE _eState
OF
  E_PROGRESS.WORKING:
    _eState                         := E_PROGRESS.DONE;
END_CASE

AxisEnable := _eState;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="AxisGearIn" Id="{93b372b7-90fa-0265-20ee-a1e0c6284158}" FolderPath="methods\">
      <Declaration><![CDATA[METHOD AxisGearIn : E_PROGRESS
VAR_INPUT
  Execute           : BOOL;
END_VAR
VAR_INST
  _eState           : E_PROGRESS;
  _rValue           : LREAL;
  _nErr,
  _nMasterIndex,
  _nMasterNcId      : UDINT;
  _stMsg            : ST_Message;


END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT Check() THEN AxisGearIn := E_PROGRESS.ERROR; RETURN; END_IF
IF NOT Execute THEN _eState := E_PROGRESS.INIT; END_IF

CASE _eState
OF
  E_PROGRESS.INIT:
    IF Execute 
    THEN
      memset(ADR(_stMsg),0,SIZEOF(_stMsg));
      _nMasterNcId                  :=
      _nMasterIndex                 :=
      _nErr                         := 0;
      _rValue                       := 0;

      IF NOT (_GearInParameter^[_nAxisIndex].nAxisIndexMaster[1] > 0) OR
         NOT (_GearInParameter^[_nAxisIndex].nAxisIndexMaster[1] < MAX_AXIS+1)
      THEN
        _nMasterIndex               := _GearInParameter^[_nAxisIndex].nAxisIndexMaster[1];

        _stMsg.eType                := E_MessageType.eMessageError;
        _stMsg.eDevice              := e_Device.Axis + TO_UINT(_AxisRef.NcToPlc.AxisId);
        _stMsg.eSubdevice           := e_Subdevice.fbGearIn;
        _stMsg.iErrorNumber         := 
        _nErr                       := 666;

        _stMsg.sText                := concat('_GearInParameter[', TO_STRING(_nAxisIndex));
        _stMsg.sText                := concat(_stMsg.sText,    '].nAxisIndexMaster[1] out of range: ');
        _stMsg.sText                := concat(_stMsg.sText, TO_STRING(_nMasterIndex));

        f_MessageSet(_stMsg);

        _eState                     := E_PROGRESS.ERROR;

      ELSE
        _eState                     := E_PROGRESS.BUSY;

        _nMasterIndex               := _GearInParameter^[_nAxisIndex].nAxisIndexMaster[1];
        _nMasterNcId                := _Info^[_nMasterIndex].nNcAxisId;

        _stMsg.eType                := E_MessageType.eMessageInfo;
        _stMsg.eDevice              := e_Device.Axis + TO_UINT(_AxisRef.NcToPlc.AxisId);
        _stMsg.eSubdevice           := e_Subdevice.fbGearIn;
        _stMsg.iErrorNumber         := 0;
  
        _stMsg.sText                := concat('GearIn BUSY: ', TO_STRING(_nMasterIndex));
        f_MessageSet(_stMsg);
      END_IF
    END_IF
END_CASE
CASE _eState
OF
  E_PROGRESS.BUSY:
    _eState                         := E_PROGRESS.STARTUP;
    _fbGearIn(
              Master                := _Axes[_nMasterIndex], 
              Slave                 := _AxisRef, 
              Execute               := FALSE, 
              RatioNumerator        := _GearInParameter^[_nAxisIndex].rGearRatio[1], 
              RatioDenominator      := 1, 
              Acceleration          := _GearInParameter^[_nAxisIndex].Acceleration, 
              Deceleration          := _GearInParameter^[_nAxisIndex].Deceleration, 
              Jerk                  := _GearInParameter^[_nAxisIndex].Jerk);
END_CASE
CASE _eState
OF
  E_PROGRESS.STARTUP:
    _fbGearIn(
              Master                := _Axes[_nMasterIndex], 
              Slave                 := _AxisRef, 
              Execute               := TRUE, 
              RatioNumerator        := _GearInParameter^[_nAxisIndex].rGearRatio[1], 
              RatioDenominator      := 1, 
              Acceleration          := _GearInParameter^[_nAxisIndex].Acceleration, 
              Deceleration          := _GearInParameter^[_nAxisIndex].Deceleration, 
              Jerk                  := _GearInParameter^[_nAxisIndex].Jerk);

    IF (_fbGearIn.Error)
    THEN
      _nErr                         := _fbGearIn.ErrorID;

      _stMsg.eType                  := E_MessageType.eMessageError;
      _stMsg.eDevice                := e_Device.Axis + TO_UINT(_AxisRef.NcToPlc.AxisId);
      _stMsg.eSubdevice             := e_Subdevice.fbGearIn;
      _stMsg.iErrorNumber           := _nErr;

      _stMsg.sText                  := concat('GearIn ERROR: ', TO_STRING(_nMasterIndex));
      f_MessageSet(_stMsg);
      _eState                       := E_PROGRESS.ERROR;

    ELSIF (_fbGearIn.InGear)
    THEN
      _eState                       := E_PROGRESS.READY;
    END_IF
END_CASE
CASE _eState
OF
  E_PROGRESS.READY:
    _eState                         := E_PROGRESS.DONE;

    _stMsg.eType                    := E_MessageType.eMessageInfo;
    _stMsg.eDevice                  := e_Device.Axis + TO_UINT(_AxisRef.NcToPlc.AxisId);
    _stMsg.eSubdevice               := e_Subdevice.fbGearIn;
    _stMsg.iErrorNumber             := 0;

    _stMsg.sText                    := concat('GearIn done: ', TO_STRING(_nMasterIndex));
    f_MessageSet(_stMsg);
END_CASE
CASE _eState
OF
  E_PROGRESS.DONE,
  E_PROGRESS.ERROR:
    _fbGearIn(
              Master                := _Axes[_nMasterIndex], 
              Slave                 := _AxisRef, 
              Execute               := FALSE);
END_CASE

AxisGearIn                          := _eState;
]]></ST>
      </Implementation>
    </Method>
    <Method Name="AxisGearInMultiMaster" Id="{2753b84c-8efc-02bd-1ed9-f32e456b6aa2}" FolderPath="methods\">
      <Declaration><![CDATA[METHOD AxisGearInMultiMaster : E_PROGRESS
VAR_INPUT
  Execute           : BOOL;
END_VAR
VAR_INST
  _eState           : E_PROGRESS;
  _rValue           : LREAL;
  _nAx,
  _nErr             : UDINT;

  _nMasterIndex,
  _nMasterNcId      : ARRAY[1..4] OF UDINT;

  _stOptions        : Tc2_MC2.ST_GearInMultiMasterOptions;
  _nSyncMode        : int;

  _stMsg            : ST_Message;


END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF NOT Check() THEN AxisGearInMultiMaster := E_PROGRESS.ERROR; RETURN; END_IF
IF NOT Execute THEN _eState := E_PROGRESS.INIT; END_IF

CASE _eState
OF
  E_PROGRESS.INIT:
    IF Execute 
    THEN
      memset(ADR(_stMsg),0,SIZEOF(_stMsg));
      memset(ADR(_nMasterNcId), 0,SIZEOF(_nMasterNcId));
      memset(ADR(_nMasterIndex),0,SIZEOF(_nMasterIndex));
      memset(ADR(_stOptions),   0,SIZEOF(_stOptions));
      _nErr                           := 0;
      _rValue                         := 0;

      IF NOT (_GearInParameter^[_nAxisIndex].nAxisIndexMaster[1] > 0) OR
         NOT (_GearInParameter^[_nAxisIndex].nAxisIndexMaster[1] <= MAX_AXIS)
      THEN
        _nMasterIndex[1]              := _GearInParameter^[_nAxisIndex].nAxisIndexMaster[1];

        _stMsg.eType                  := E_MessageType.eMessageError;
        _stMsg.eDevice                := e_Device.Axis + TO_UINT(_AxisRef.NcToPlc.AxisId);
        _stMsg.eSubdevice             := e_Subdevice.fbGearInMultiMaster;
        _stMsg.iErrorNumber           := 
        _nErr                         := 666;

        _stMsg.sText                  := concat('_GearInParameter[', TO_STRING(_nAxisIndex));
        _stMsg.sText                  := concat(_stMsg.sText,    '].nAxisIndexMaster[1] out of range: ');
        _stMsg.sText                  := concat(_stMsg.sText, TO_STRING(_nMasterIndex[1]));
        f_MessageSet(_stMsg);

        _eState                       := E_PROGRESS.ERROR;

      ELSE
        _eState                       := E_PROGRESS.BUSY;

        _stOptions.AdvancedSlaveDynamics  := _GearInParameter^[_nAxisIndex].bAdvancedSlaveDynamics;
        _nSyncMode                        := _GearInParameter^[_nAxisIndex].nMultiMasterSyncMode;

        _nMasterIndex[1]              := _GearInParameter^[_nAxisIndex].nAxisIndexMaster[1];
        _nMasterNcId[1]               := _Info^[_nMasterIndex[1]].nNcAxisId;
        memcpy(ADR(_stOptions.SyncMode), ADR(_nSyncMode), SIZEOF(_stOptions.SyncMode));

        _fbGearInMultiMaster.Master1  := _Axes[_nMasterIndex[1]];   // assign AXIS_REF of master

        _stMsg.eType                  := E_MessageType.eMessageInfo;
        _stMsg.eDevice                := e_Device.Axis + TO_UINT(_AxisRef.NcToPlc.AxisId);
        _stMsg.eSubdevice             := e_Subdevice.fbGearInMultiMaster;
        _stMsg.iErrorNumber           := _eState;

        _stMsg.sText                  := concat('_GearInParameter[', TO_STRING(_nAxisIndex));
        _stMsg.sText                  := concat(_stMsg.sText,    '] MultiMaster check: ');
        _stMsg.sText                  := concat(_stMsg.sText, TO_STRING(_nMasterIndex[1]));
        f_MessageSet(_stMsg);

        _nAx                          := 2; // next master index to check
      END_IF
    END_IF
END_CASE
CASE _eState
OF
  E_PROGRESS.BUSY:
    IF NOT (_GearInParameter^[_nAxisIndex].nAxisIndexMaster[_nAx] > 0) OR
       NOT (_GearInParameter^[_nAxisIndex].nAxisIndexMaster[_nAx] < MAX_AXIS+1)
    THEN
      // next entry not valid
      // Master1 is already checked --> let's get ready
      _eState                         := E_PROGRESS.READY;
    ELSE
      _eState                         := E_PROGRESS.STARTUP;

      _nMasterIndex[_nAx]             := _GearInParameter^[_nAxisIndex].nAxisIndexMaster[_nAx];
      _nMasterNcId [_nAx]             := _Info^[_nMasterIndex[_nAx]].nNcAxisId;

      _stMsg.eType                    := E_MessageType.eMessageInfo;
      _stMsg.eDevice                  := e_Device.Axis + TO_UINT(_AxisRef.NcToPlc.AxisId);
      _stMsg.eSubdevice               := e_Subdevice.fbGearInMultiMaster;
      _stMsg.iErrorNumber             := _eState;

      _stMsg.sText                    := concat('_GearInParameter[', TO_STRING(_nAxisIndex));
      _stMsg.sText                    := concat(_stMsg.sText,    '] MultiMaster check: ');
      _stMsg.sText                    := concat(_stMsg.sText, TO_STRING(_nMasterIndex[_nAx]));
      f_MessageSet(_stMsg);
    END_IF

    _fbGearInMultiMaster(
                      Slave           := _AxisRef, 
                      Enable          := FALSE, 
                      GearRatio1      := _GearInParameter^[_nAxisIndex].rGearRatio[1], 
                      GearRatio2      := _GearInParameter^[_nAxisIndex].rGearRatio[2], 
                      GearRatio3      := _GearInParameter^[_nAxisIndex].rGearRatio[3], 
                      GearRatio4      := _GearInParameter^[_nAxisIndex].rGearRatio[4], 
                      Acceleration    := _GearInParameter^[_nAxisIndex].Acceleration, 
                      Deceleration    := _GearInParameter^[_nAxisIndex].Deceleration, 
                      Jerk            := _GearInParameter^[_nAxisIndex].Jerk,
                      Options         := _stOptions);
END_CASE
CASE _eState
OF
  E_PROGRESS.STARTUP:
    CASE _nAx
    OF
      2:
        _nAx                          := _nAx + 1;
        _eState                       := E_PROGRESS.BUSY;
        _fbGearInMultiMaster.Master2  := _Axes[_nMasterIndex[2]];   // assign AXIS_REF of master

      3:
        _nAx                          := _nAx + 1;
        _eState                       := E_PROGRESS.BUSY;
        _fbGearInMultiMaster.Master3  := _Axes[_nMasterIndex[3]];   // assign AXIS_REF of master

      4:
        _nAx                          := _nAx + 1;
        _eState                       := E_PROGRESS.BUSY;
        _fbGearInMultiMaster.Master4  := _Axes[_nMasterIndex[4]];   // assign AXIS_REF of master
    ELSE
      _eState                         := E_PROGRESS.READY;
    END_CASE
END_CASE
CASE _eState
OF
  E_PROGRESS.READY:
    _fbGearInMultiMaster(
                      Slave           := _AxisRef, 
                      Enable          := TRUE, 
                      GearRatio1      := _GearInParameter^[_nAxisIndex].rGearRatio[1], 
                      GearRatio2      := _GearInParameter^[_nAxisIndex].rGearRatio[2], 
                      GearRatio3      := _GearInParameter^[_nAxisIndex].rGearRatio[3], 
                      GearRatio4      := _GearInParameter^[_nAxisIndex].rGearRatio[4], 
                      Acceleration    := _GearInParameter^[_nAxisIndex].Acceleration, 
                      Deceleration    := _GearInParameter^[_nAxisIndex].Deceleration, 
                      Jerk            := _GearInParameter^[_nAxisIndex].Jerk);

    IF (_fbGearInMultiMaster.Error)
    THEN
      _nErr                           := _fbGearInMultiMaster.ErrorID;
      _stMsg.eType                    := E_MessageType.eMessageError;
      _stMsg.eDevice                  := e_Device.Axis + TO_UINT(_AxisRef.NcToPlc.AxisId);
      _stMsg.eSubdevice               := e_Subdevice.fbGearInMultiMaster;
      _stMsg.iErrorNumber             := _nErr;

      _stMsg.sText                    := concat(TO_STRING(_eCtrl),'[');
      _stMsg.sText                    := concat(_stMsg.sText, TO_STRING(_nAxisIndex));
      _stMsg.sText                    := concat(_stMsg.sText, ']');
      _stMsg.sText                    := concat('fbGearInMultiMaster error: ', TO_STRING(_nErr));
      f_MessageSet(_stMsg);
      _eState                         := E_PROGRESS.ERROR;

    ELSIF (_fbGearIn.InGear)
    THEN
      _eState                         := E_PROGRESS.WORKING;
    END_IF

END_CASE
CASE _eState
OF
  E_PROGRESS.WORKING:
    // now GearRatio[x] can be altered from outside
    // axis function waits here for further instructions from outside world
    // _stCtrl[_nAxisIndex].eProgress = e_Progress.DONE must be used to leave this axis function
    _fbGearInMultiMaster(
                      Slave           := _AxisRef, 
                      Enable          := TRUE, 
                      GearRatio1      := _GearInParameter^[_nAxisIndex].rGearRatio[1], 
                      GearRatio2      := _GearInParameter^[_nAxisIndex].rGearRatio[2], 
                      GearRatio3      := _GearInParameter^[_nAxisIndex].rGearRatio[3], 
                      GearRatio4      := _GearInParameter^[_nAxisIndex].rGearRatio[4], 
                      Acceleration    := _GearInParameter^[_nAxisIndex].Acceleration, 
                      Deceleration    := _GearInParameter^[_nAxisIndex].Deceleration, 
                      Jerk            := _GearInParameter^[_nAxisIndex].Jerk);

    IF (_fbGearInMultiMaster.Error)
    THEN
      _nErr                           := _fbGearInMultiMaster.ErrorID;
      _stMsg.eType                    := E_MessageType.eMessageError;
      _stMsg.eDevice                  := e_Device.Axis + TO_UINT(_AxisRef.NcToPlc.AxisId);
      _stMsg.eSubdevice               := e_Subdevice.fbGearInMultiMaster;
      _stMsg.iErrorNumber             := _nErr;

      _stMsg.sText                    := concat(TO_STRING(_eCtrl),'[');
      _stMsg.sText                    := concat(_stMsg.sText, TO_STRING(_nAxisIndex));
      _stMsg.sText                    := concat(_stMsg.sText, ']');
      _stMsg.sText                    := concat('fbGearInMultiMaster error: ', TO_STRING(_nErr));
      f_MessageSet(_stMsg);
      _eState                         := E_PROGRESS.ERROR;

    ELSIF (_Ctrl^[_nAxisIndex].eProgress = E_PROGRESS.DONE)
    THEN
      // when Ctrl decides to leave; make finishing move and be done
      _eState                         := E_PROGRESS.FINISHING;
    END_IF
END_CASE
CASE _eState
OF
  E_PROGRESS.FINISHING:
    _eState                           := E_PROGRESS.DONE;

    _stMsg.eType                      := E_MessageType.eMessageInfo;
    _stMsg.eDevice                    := e_Device.Axis + TO_UINT(_AxisRef.NcToPlc.AxisId);
    _stMsg.eSubdevice                 := e_Subdevice.fbGearInMultiMaster;
    _stMsg.iErrorNumber               := _eState;

    _stMsg.sText                      := concat(TO_STRING(_eCtrl), ': ');
    _stMsg.sText                      := concat(_stMsg.sText, TO_STRING(_eState));
    f_MessageSet(_stMsg);
END_CASE

AxisGearInMultiMaster                 := _eState;
]]></ST>
      </Implementation>
    </Method>
    <Property Name="AxisGearInParameter" Id="{b3665f5f-7d61-0534-18c3-fc4c139bfd64}" FolderPath="properties\">
      <Declaration><![CDATA[PROPERTY AxisGearInParameter : pointer to ARRAY[1..PLC_CONSTANT.MAX_AXIS] OF ST_GEAR_IN]]></Declaration>
